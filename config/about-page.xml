	</listeners>
	<test name="About Page">
		<classes>
			<class name="BRP.TestAboutPage" />
		</classes>
	</test>
	<test name="Home Page">
		<classes>
			<class name="BRP.TestHomePage" />
		</classes>
	</test> <!--Test -->
</suite> <!--Suite -->
